        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Patch Notes"
        Height="500"
        Width="700"
        ResizeMode="CanMinimize"
        WindowStartupLocation="CenterOwner">
    <Grid Margin="10">
        <ScrollViewer VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="PatchNotesPanel" />
        </ScrollViewer>
    </Grid>
</Window>